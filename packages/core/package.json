{
	"name": "@elekcsv/core",
	"version": "1.0.0",
	"description": "CSV parsing, validation, and transformation engine",
	"type": "module",
	"license": "Apache-2.0",
	"author": "Kayra Berk Tuncer",
	"repository": {
		"type": "git",
		"url": "https://github.com/k61b/elekcsv.git",
		"directory": "packages/core"
	},
	"homepage": "https://github.com/k61b/elekcsv",
	"bugs": {
		"url": "https://github.com/k61b/elekcsv/issues"
	},
	"keywords": [
		"csv",
		"parser",
		"validator",
		"transform",
		"locale",
		"typescript"
	],
	"engines": {
		"node": ">=18"
	},
	"sideEffects": false,
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		}
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"test": "bun test",
		"test:watch": "bun test --watch"
	},
	"devDependencies": {
		"tsup": "^8.3.5",
		"typescript": "^5.7.2"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org"
	}
}
