{
	"name": "@elekcsv/solid",
	"version": "1.0.2",
	"description": "SolidJS signals for elek CSV import",
	"type": "module",
	"license": "Apache-2.0",
	"author": "Kayra Berk Tuncer",
	"repository": {
		"type": "git",
		"url": "https://github.com/k61b/elekcsv.git",
		"directory": "packages/solid"
	},
	"homepage": "https://github.com/k61b/elekcsv",
	"bugs": {
		"url": "https://github.com/k61b/elekcsv/issues"
	},
	"keywords": [
		"csv",
		"import",
		"solid",
		"signals",
		"headless",
		"typescript"
	],
	"engines": {
		"node": ">=18"
	},
	"sideEffects": false,
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		}
	},
	"files": [
		"dist",
		"README.md"
	],
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"test": "bun test",
		"test:watch": "bun test --watch"
	},
	"dependencies": {
		"@elekcsv/core": "^1.1.1"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org"
	},
	"peerDependencies": {
		"solid-js": ">=1"
	},
	"devDependencies": {
		"solid-js": "^1.9.3",
		"tsup": "^8.3.5",
		"typescript": "^5.7.2"
	}
}
